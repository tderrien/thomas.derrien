	pmid = {28190401},
	note = {Publisher: Genome Biology},
	keywords = {transcriptome, correspondence, ac, david, earlham, gene duplication, genome sequence, Genome sequence, hemiptera, hogenhout, jic, myzus persicae, Myzus persicae, parasite, plasticity, Plasticity, rna interference, rnai, sap-feeding insects, saskia, swarbreck, Transcri, uk},
